<div class="grouping">
    <% keys.forEach(function(item){  %>
        <p>
            <%= item %>
        </p>
        <% data[item].map(function(post){  %>
        <a href="<%- config.root %><%- post.path %>">
            <span><%- (post.title || "Untitled").replace(/[<>&"]/g,function(c){
                    return {'<':'&lt;','>':'&gt;','&':'&amp;','"':'&quot;'}[c];
                }) %></span>
            <time><%= post.date.format('MM-DD') %></time>
        </a>
        <% }) %>
        <% }) %>
</div>